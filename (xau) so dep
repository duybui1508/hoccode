#include <bits/stdc++.h>
using namespace std;
#define ll long long
bool check(string s){
   for(int i=0; i< s.size(); i+=){
    if(abs((s[i]- '0')-(s[i-1]-'0')) !=1 )
    return false;}
    return true;
}
int main(){
   string s;
   getline(cin, s);
  if(check(s)) cout << "YES\n";
  else cout << "NO\n";
}
