#include <bits/stdc++.h>
using namespace std;
#define ll long long
int fp(int a[], int l, int r, int x){
   int ans=-1;
   while(l<=r){
      int m=(l+r)/2;
      if(a[m]==x){
         ans=m;
         r=m-1;
      }
      else if(a[m]<x){
         l=m+1;
      }
      else{
         r=m-1;
      }
   }
   return ans;
}
int main(){
   int n; cin>>n;
   vector<int> a(n);
   for(int i=0; i<n; i++){
      cin >> a[i];
   }
   int x; cin >>x;
   sort(a.begin(), a.end());

  cout << fp(a.data(),0, n-1, x) <<endl;
}
