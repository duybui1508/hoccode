#include <bits/stdc++.h>
using namespace std;
#define ll long long
int nt(int n){
   if( n<2 ) return 0;
    for(int i=2; i<=sqrt(n); i++){
      if(n%i==0) return 0;
    }
    return 1;
}
int main(){
   int n,m;
   cin >> n >> m ;
   int a[50][50];
   for(int i=0; i<n; i++){
      for(int j=0; j<m; j++){
         cin >> a[i][j];
       
      }
   }
   set <int> se;
   for(int i=0; i<n; i++){
      if(nt(a[i][i])) se.insert(a[i][i]);
      if(nt(a[i][n-i-1])) se.insert(a[i][n-i-1]);

   }
 
   cout << se.size() << endl;
}
