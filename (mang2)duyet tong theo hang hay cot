#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
   int n,m;
   cin >> n >> m ;
   int a[50][50];
   for(int i=0; i<n; i++){
      for(int j=0; j<m; j++){
         cin >> a[i][j];
       
      }
   }
   for(int i=0; i<n; i++){
      ll tong=0;
      for(int j=0; j<m; j++){
           tong += a[i][j];
         }
      }
      cout << endl; //duyet theo hang
   
   for(int i=0; i<m i++){
      ll tong=0;
      for(int j=0; j<n; j++){
         tong += a[j][i];
         } //duyet theo cot.
      }
   
   }
